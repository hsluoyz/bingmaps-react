{
  "name": "@hsluoyz/bingmaps-react",
  "description": "An easy to use Bing Maps component for React apps",
  "author": {
    "name": "Miles Pratt",
    "email": "miles@thoughtpowder.com",
    "url": "https://milespratt.com"
  },
  "keywords": [
    "bing",
    "maps",
    "bingmaps",
    "bing maps",
    "react bing maps",
    "microsoft bingmaps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/milespratt/bingmaps-react"
  },
  "version": "0.4.10",
  "private": false,
  "module": "dist/BingMapsReact.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/components/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/src/index.js",
      "!**/src/serviceWorker.js"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "testCoverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel src/components -d dist --copy-files && npm publish"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/preset-react": "^7.12.7",
    "react-test-renderer": "^17.0.1"
  }
}
